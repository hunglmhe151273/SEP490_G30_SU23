@if (TempData["success"] != null)
{
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/mgmt-lib/plugins/notifications/js/lobibox.min.js"></script>
    <script src="~/mgmt-lib/plugins/notifications/js/notifications.min.js"></script>
    <script src="~/mgmt-lib/plugins/notifications/js/notification-custom-script.js"></script>
    <script type="text/javascript">
        round_success_noti('@TempData["success"]');
    </script>
}
@if (TempData["error"] != null)
{
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/mgmt-lib/plugins/notifications/js/lobibox.min.js"></script>
    <script src="~/mgmt-lib/plugins/notifications/js/notifications.min.js"></script>
    <script src="~/mgmt-lib/plugins/notifications/js/notification-custom-script.js"></script>
    <script type="text/javascript">
        round_error_noti('@TempData["error"]');
    </script>
}
