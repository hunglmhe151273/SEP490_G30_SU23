                                                    </div>
                                                </li>
                                                <li>
                                                    <div class="total-cart-pro">
                                                        @foreach (CartDetail detail in cartDetail)
                                                        {
                                                            int price = (int)(detail.Product.RetailPrice.Value * (1 - detail.Product.RetailDiscount.Value / 100)
                                                                * detail.Quantity.Value);
                                                            total += price;

                                                            <!-- single-cart -->
                                                            <div class="single-cart clearfix">
                                                                <div class="cart-img f-left">
                                                                    <a href="#">
                                                                        <img src="@imageRoot/images/img/@thumbnails[detail.ProductId]" alt="Cart Product"
                                                                             style="width: 100px; aspect-ratio: 100/111; object-fit: contain;" />
                                                                    </a>
                                                                    @*<div class="del-icon">
                                                                        <a href="#">
