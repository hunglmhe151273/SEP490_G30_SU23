@model IEnumerable<VBookHaven.Models.ShippingInfo>
@{
    ViewData["Title"] = "ShipInfo";
    Layout = "~/Views/Shared/_LayoutCustomer.cshtml";
}

<h1>ShipInfo</h1>
<table class="table bg-white">
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.CustomerName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ShipAddress)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Status)
            </td>
            <td>
                <a asp-action="EditShipInfo" asp-route-shipInfoId="@item.ShipInfoId">Chỉnh sửa</a>@* |
            <a asp-action="Details" asp-route-id="@item.CategoryId">Chi tiết</a>*@@* |
            <a asp-action="Delete" asp-route-id="@item.CategoryId">Delete</a>*@
            </td>
        </tr>
    }
</table>